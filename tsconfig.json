{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "flopkart-cart": [
        "dist/flopkart-cart"
      ],
      "flopkart-cart/*": [
        "dist/flopkart-cart/*"
      ],
      "flopkart-home": [
        "dist/flopkart-home"
      ],
      "flopkart-home/*": [
        "dist/flopkart-home/*"
      ]
    }
  }
}